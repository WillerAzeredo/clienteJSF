<pretty-config xmlns="http://ocpsoft.com/prettyfaces/3.3.2" 
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
               xsi:schemaLocation="http://ocpsoft.com/prettyfaces/3.3.2
               http://ocpsoft.com/xml/ns/prettyfaces/ocpsoft-pretty-faces-3.3.2.xsd">
    <rewrite match="^/(.*)/$" substitute="/$1" redirect="301" />
    <url-mapping id="index">
        <pattern value="/" />
        <view-id value="/index.jsf" />
    </url-mapping>
    <url-mapping id="cliente">
        <pattern value="/cliente" />
        <view-id value="/cliente.jsf" />
    </url-mapping>
</pretty-config>